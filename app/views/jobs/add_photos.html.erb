<h1>Add photos to job</h1>

<!-- Renders validation error messages  -->
<% if @job.errors.any? %>
    <ul>
        <% @job.errors.full_messages.each do |msg| %>
        <li class="error"> <%= msg %></li>
        <% end  %>
    </ul>
<% end  %>

<%= form_for @job, :html => {:multipart => true} do |f| %>

    <%= f.label "Job photos" %>
    <%= f.file_field :images, :multiple => true %>

    <%= f.submit %>
<% end %>
